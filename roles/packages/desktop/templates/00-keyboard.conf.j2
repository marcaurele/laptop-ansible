# {{ ansible_managed }}
Section "InputClass"
  Identifier "system-keyboard"
  MatchIsKeyboard "on"
  Option "XkbLayout" "us,ch"
  Option "XkbModel" "pc105"
  Option "XkbVariant" "altgr-intl,fr"
  Option "XkbOptions" "grp:win_space_toggle"
  Option "XkbOptions" "ctrl:nocaps"
  #Option "XkbOptions" "eurosign:e"
EndSection