# {{ ansible_managed }}
Section "InputClass"
  Identifier "foostan Corne Keyboard"
  Driver "libinput"
  MatchIsKeyboard "on"
  MatchProduct "foostan Corne"
  #MatchUSBID "4653:0001"

  Option "XkbLayout" "us,ch"
  Option "XkbVariant" "altgr-intl,fr"
  Option "XkbOptions" ",ctrl:nocaps,grp:win_space_toggle"
EndSection
